{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "blockName": "tpgb/tp-container",
  "title": "Container - Core Attributes",
  "description": "Core content and structure attributes",
  "attributes": {
    "block_id": {
      "type": "string",
      "required": true,
      "description": "Unique 4-character hex identifier for the block"
    },
    "anchor": {
      "type": "string",
      "description": "HTML anchor/ID for scroll linking"
    },
    "className": {
      "type": "string",
      "default": "",
      "description": "Additional CSS class names"
    },
    "columns": {
      "type": "number",
      "default": "",
      "description": "Number of columns (auto-calculated based on inner blocks)"
    },
    "contentWidth": {
      "type": "string",
      "default": "wide",
      "enum": [
        "wide",
        "full"
      ],
      "description": "Container width mode: wide (contained) or full (edge-to-edge)"
    },
    "contwidFull": {
      "type": "string",
      "default": "",
      "description": "Full width container configuration"
    },
    "containerWide": {
      "$ref": "definitions://responsive-unit"
    },
    "containerFull": {
      "$ref": "definitions://responsive-unit"
    },
    "colDir": {
      "type": "string",
      "default": "",
      "description": "Column direction override"
    },
    "sectionWidth": {
      "type": "string",
      "default": "boxed",
      "enum": [
        "boxed",
        "full"
      ],
      "description": "Section width mode"
    },
    "gutterSpace": {
      "$ref": "definitions://responsive-unit"
    },
    "tagName": {
      "type": "string",
      "default": "div",
      "enum": [
        "div",
        "section",
        "article",
        "header",
        "footer",
        "main",
        "aside"
      ],
      "description": "HTML tag for semantic structure"
    },
    "liveCopy": {
      "type": "boolean",
      "default": false,
      "description": "Enable live copy mode for template replication"
    },
    "currentID": {
      "type": "number",
      "default": "",
      "description": "Current post/page ID for live copy"
    },
    "customClass": {
      "type": "string",
      "default": "",
      "description": "Custom CSS classes"
    },
    "customId": {
      "type": "string",
      "default": "",
      "description": "Custom HTML ID"
    },
    "customCss": {
      "type": "string",
      "default": "",
      "description": "Custom CSS rules for this block"
    },
    "shapeTWidth": {
      "$ref": "definitions://responsive-unit"
    },
    "shapeTFlip": {
      "type": "boolean",
      "default": false,
      "description": "Flip top shape divider horizontally"
    },
    "shapeTInvert": {
      "type": "boolean",
      "default": false,
      "description": "Invert top shape divider vertically"
    },
    "shapeTFront": {
      "type": "boolean",
      "default": false,
      "description": "Bring top shape divider to front (above content)"
    },
    "shapeBWidth": {
      "$ref": "definitions://responsive-unit"
    },
    "shapeBFlip": {
      "type": "boolean",
      "default": false,
      "description": "Flip bottom shape divider horizontally"
    },
    "shapeBInvert": {
      "type": "boolean",
      "default": false,
      "description": "Invert bottom shape divider vertically"
    },
    "shapeBFront": {
      "type": "boolean",
      "default": false,
      "description": "Bring bottom shape divider to front"
    },
    "HideDesktop": {
      "type": "boolean",
      "default": false,
      "description": "Hide container on desktop (1201px+)"
    },
    "HideTablet": {
      "type": "boolean",
      "default": false,
      "description": "Hide container on tablet (768-1200px)"
    },
    "HideMobile": {
      "type": "boolean",
      "default": false,
      "description": "Hide container on mobile (767px and below)"
    },
    "animdur": {
      "type": "string",
      "default": "3",
      "description": "Pro: Animation duration in seconds for color transitions",
      "proFeature": true
    },
    "animDelay": {
      "type": "string",
      "default": "",
      "description": "Pro: Animation delay in seconds",
      "proFeature": true
    },
    "crativeImg": {
      "type": "object",
      "default": {
        "url": "",
        "Id": ""
      },
      "description": "Pro: Creative background image upload",
      "proFeature": true
    },
    "deepimgAtta": {
      "type": "object",
      "default": {
        "md": "",
        "sm": "",
        "xs": ""
      },
      "description": "Pro: Deep image background attachment",
      "responsive": true,
      "proFeature": true
    },
    "deepimgRepeat": {
      "type": "object",
      "default": {
        "md": "",
        "sm": "",
        "xs": ""
      },
      "description": "Pro: Deep image background repeat",
      "responsive": true,
      "proFeature": true
    },
    "imgeffect": {
      "type": "string",
      "default": "",
      "enum": [
        "",
        "tilt",
        "mouse_move"
      ],
      "description": "Pro: Image effect interaction type (tilt or mouse parallax)",
      "proFeature": true
    },
    "intensity": {
      "type": "string",
      "default": "30",
      "description": "Pro: Effect intensity (0-100)",
      "proFeature": true
    },
    "perspective": {
      "type": "string",
      "default": "1000",
      "description": "Pro: 3D perspective for tilt effect",
      "proFeature": true
    },
    "Scale": {
      "type": "string",
      "default": "1",
      "description": "Pro: Scale multiplier for effects",
      "proFeature": true
    },
    "inverted": {
      "type": "boolean",
      "default": false,
      "description": "Pro: Invert effect direction",
      "proFeature": true
    },
    "scrollPara": {
      "type": "boolean",
      "default": false,
      "description": "Pro: Enable scroll parallax effect",
      "proFeature": true
    },
    "kburnseffect": {
      "type": "boolean",
      "default": false,
      "description": "Pro: Enable Ken Burns zoom/pan effect",
      "proFeature": true
    },
    "Kbeffctdir": {
      "type": "string",
      "default": "normal",
      "enum": [
        "normal",
        "reverse"
      ],
      "description": "Pro: Ken Burns effect direction",
      "proFeature": true
    },
    "effctDure": {
      "type": "string",
      "default": "14",
      "description": "Pro: Effect duration in seconds",
      "proFeature": true
    },
    "respoImg": {
      "type": "boolean",
      "default": false,
      "description": "Pro: Enable responsive background images",
      "proFeature": true
    },
    "tabImage": {
      "$ref": "definitions://background"
    },
    "mobileImg": {
      "$ref": "definitions://background"
    },
    "movedir": {
      "type": "string",
      "default": "right",
      "enum": [
        "left",
        "right",
        "top",
        "bottom"
      ],
      "description": "Pro: Movement direction for animated backgrounds",
      "proFeature": true
    },
    "trasispeed": {
      "type": "string",
      "default": "30",
      "description": "Pro: Transition speed for moving backgrounds",
      "proFeature": true
    },
    "videosour": {
      "type": "string",
      "default": "youtube",
      "enum": [
        "youtube",
        "vimeo",
        "self-hosted"
      ],
      "description": "Pro: Video background source type",
      "proFeature": true
    },
    "mp4Url": {
      "type": "string",
      "default": "",
      "description": "Pro: MP4 video URL for self-hosted video background",
      "proFeature": true
    },
    "WebMUrl": {
      "type": "string",
      "default": "",
      "description": "Pro: WebM video URL for self-hosted video background",
      "proFeature": true
    },
    "youtubeId": {
      "type": "string",
      "default": "",
      "description": "Pro: YouTube video ID for video background",
      "proFeature": true
    },
    "vimeoId": {
      "type": "string",
      "default": "",
      "description": "Pro: Vimeo video ID for video background",
      "proFeature": true
    },
    "iframeTitle": {
      "type": "string",
      "default": "",
      "description": "Pro: Accessible iframe title for video embed",
      "proFeature": true
    },
    "videoloop": {
      "type": "boolean",
      "default": false,
      "description": "Pro: Loop video background",
      "proFeature": true
    },
    "videoMute": {
      "type": "boolean",
      "default": false,
      "description": "Pro: Mute video background audio",
      "proFeature": true
    },
    "videoImg": {
      "type": "object",
      "default": {
        "url": ""
      },
      "description": "Pro: Video poster/fallback image",
      "proFeature": true
    },
    "parallax": {
      "type": "boolean",
      "default": false,
      "description": "Pro: Enable parallax effect on middle layer images",
      "proFeature": true
    },
    "rowImgs": {
      "type": "array",
      "default": [],
      "description": "Pro: Legacy parallax images array",
      "proFeature": true
    },
    "midimgList": {
      "type": "array",
      "description": "Pro: Middle layer parallax images with extensive positioning and magic scroll effects",
      "repeaterField": {
        "parallaxImg": {
          "type": "object",
          "default": {
            "url": ""
          },
          "description": "Parallax image upload"
        },
        "DleftAuto": {
          "type": "boolean",
          "default": false,
          "description": "Auto left positioning (desktop)"
        },
        "leftPos": {
          "type": "string",
          "default": "20",
          "description": "Left position percentage"
        },
        "DrightAuto": {
          "type": "boolean",
          "default": false,
          "description": "Auto right positioning (desktop)"
        },
        "righttPos": {
          "type": "string",
          "description": "Right position percentage"
        },
        "DtopAuto": {
          "type": "boolean",
          "default": false,
          "description": "Auto top positioning (desktop)"
        },
        "topPos": {
          "type": "string",
          "default": "25",
          "description": "Top position percentage"
        },
        "DbottomAuto": {
          "type": "boolean",
          "default": false,
          "description": "Auto bottom positioning (desktop)"
        },
        "bottomPos": {
          "type": "string",
          "description": "Bottom position percentage"
        },
        "DimgWidth": {
          "type": "string",
          "default": "150",
          "description": "Desktop image width (px)"
        },
        "tpgbMagicScroll": {
          "type": "boolean",
          "default": false,
          "description": "Enable magic scroll animations"
        },
        "MSScrollOpt": {
          "type": "object",
          "description": "Magic scroll options (trigger, duration, offset)"
        },
        "MSVertical": {
          "type": "object",
          "description": "Vertical movement speed and reverse"
        },
        "MSHorizontal": {
          "type": "object",
          "description": "Horizontal movement speed and reverse"
        },
        "MSOpacity": {
          "type": "object",
          "description": "Opacity animation speed and reverse"
        },
        "MSRotate": {
          "type": "object",
          "description": "Rotation animation (X/Y/Z axis)"
        },
        "MSScale": {
          "type": "object",
          "description": "Scale animation (X/Y/Z axis)"
        },
        "MSSkew": {
          "type": "object",
          "description": "Skew animation (X/Y axis)"
        },
        "borderR": {
          "type": "array",
          "description": "Border radius animation"
        },
        "MsadvOption": {
          "type": "array",
          "description": "Advanced animation options (repeat, easing, delay, timing)"
        },
        "MSDevelop": {
          "type": "boolean",
          "default": false,
          "description": "Magic scroll developer mode"
        },
        "devName": {
          "type": "string",
          "default": "",
          "description": "Developer identifier"
        },
        "MSSticky": {
          "type": "boolean",
          "default": false,
          "description": "Sticky positioning for magic scroll"
        },
        "Effectin": {
          "type": "string",
          "default": "",
          "description": "Entrance effect"
        },
        "imgOpa": {
          "type": "string",
          "default": "",
          "description": "Image opacity"
        },
        "ImgZind": {
          "type": "string",
          "default": "",
          "description": "Image z-index"
        },
        "resVisib": {
          "type": "boolean",
          "default": false,
          "description": "Enable responsive visibility"
        },
        "desHide": {
          "type": "boolean",
          "default": false,
          "description": "Hide on desktop"
        },
        "tabHide": {
          "type": "boolean",
          "default": false,
          "description": "Hide on tablet"
        },
        "moHide": {
          "type": "boolean",
          "default": false,
          "description": "Hide on mobile"
        },
        "imgSize": {
          "type": "string",
          "default": "",
          "description": "Image size preset"
        },
        "imgDire": {
          "type": "string",
          "default": "",
          "description": "Image direction/alignment"
        },
        "modImgeff": {
          "type": "string",
          "default": "",
          "description": "Modern image effect"
        },
        "imgeffdur": {
          "type": "string",
          "description": "Image effect duration"
        },
        "tranOrigin": {
          "type": "string",
          "default": "center center",
          "description": "Transform origin"
        }
      },
      "proFeature": true
    },
    "transieffect": {
      "type": "string",
      "default": "fade2",
      "description": "Pro: Transition effect for slideshow backgrounds",
      "proFeature": true
    },
    "transdur": {
      "type": "string",
      "default": "3000",
      "description": "Pro: Transition duration in milliseconds",
      "proFeature": true
    },
    "slidetime": {
      "type": "string",
      "default": "2000",
      "description": "Pro: Slide display time in milliseconds",
      "proFeature": true
    },
    "textureoly": {
      "type": "string",
      "default": "none",
      "description": "Pro: Texture overlay pattern",
      "proFeature": true
    },
    "scrollchg": {
      "type": "string",
      "default": "no",
      "enum": [
        "no",
        "yes"
      ],
      "description": "Pro: Background changes on scroll",
      "proFeature": true
    },
    "scrolltra": {
      "type": "string",
      "default": "0.7",
      "description": "Pro: Scroll transparency value",
      "proFeature": true
    },
    "midOption": {
      "type": "string",
      "default": "",
      "enum": [
        "",
        "canvas",
        "automove-img"
      ],
      "description": "Pro: Middle layer option (canvas particles or auto-moving images)",
      "proFeature": true
    },
    "canvasSty": {
      "type": "string",
      "default": "style-1",
      "description": "Pro: Canvas particle style preset",
      "proFeature": true
    },
    "particleList": {
      "type": "array",
      "default": [
        {
          "_key": "0",
          "bubbleColor": "#8072fc"
        },
        {
          "_key": "1",
          "bubbleColor": "#6fc784"
        },
        {
          "_key": "2",
          "bubbleColor": "#ff5a6e"
        }
      ],
      "description": "Pro: Particle colors repeater for canvas background",
      "repeaterField": {
        "bubbleColor": {
          "type": "string",
          "default": "",
          "description": "Particle/bubble color"
        }
      },
      "proFeature": true
    },
    "canShape": {
      "type": "string",
      "default": "circle",
      "enum": [
        "circle",
        "square",
        "triangle"
      ],
      "description": "Pro: Canvas particle shape",
      "proFeature": true
    },
    "ctmJson": {
      "type": "string",
      "default": "",
      "description": "Pro: Custom particles.js JSON configuration",
      "proFeature": true
    },
    "textureImg": {
      "type": "object",
      "default": {
        "url": "",
        "id": ""
      },
      "description": "Pro: Texture overlay image upload",
      "proFeature": true
    },
    "teximgAtta": {
      "type": "object",
      "default": {
        "md": "",
        "sm": "",
        "xs": ""
      },
      "description": "Pro: Texture image background attachment",
      "responsive": true,
      "proFeature": true
    },
    "teximgRepeat": {
      "type": "object",
      "default": {
        "md": "",
        "sm": "",
        "xs": ""
      },
      "description": "Pro: Texture image background repeat",
      "responsive": true,
      "proFeature": true
    },
    "wrapLink": {
      "type": "boolean",
      "default": false,
      "description": "Pro: Wrap entire container in a clickable link",
      "proFeature": true
    },
    "rowUrl": {
      "$ref": "definitions://link"
    },
    "showchild": {
      "type": "boolean",
      "default": false,
      "description": "Show child visibility controls"
    },
    "contSticky": {
      "type": "object",
      "default": {
        "md": false
      },
      "description": "Pro: Enable sticky positioning (responsive)",
      "responsive": true,
      "proFeature": true
    },
    "contOverlays": {
      "type": "boolean",
      "default": false,
      "description": "Pro: Enable container overlays",
      "proFeature": true
    },
    "constType": {
      "type": "string",
      "default": "normal-sticky",
      "enum": [
        "normal-sticky",
        "scroll-view"
      ],
      "description": "Pro: Sticky container type",
      "proFeature": true
    },
    "sanimaType": {
      "type": "string",
      "default": "none",
      "enum": [
        "none",
        "slide",
        "fade"
      ],
      "description": "Pro: Sticky animation type",
      "proFeature": true
    },
    "backFilter": {
      "type": "object",
      "default": {
        "openFilter": false,
        "isbackdrop": true
      },
      "description": "Pro: Backdrop filter effects (blur, brightness, etc.)",
      "proFeature": true
    },
    "scupSticky": {
      "type": "boolean",
      "default": false,
      "description": "Pro: Scroll up sticky behavior",
      "proFeature": true
    },
    "stTransdur": {
      "type": "string",
      "default": "0.3",
      "description": "Pro: Sticky transition duration in seconds",
      "proFeature": true
    },
    "conPosi": {
      "type": "object",
      "description": "Pro: Container positioning settings",
      "proFeature": true
    }
  },
  "required": [
    "block_id"
  ],
  "editingGuidelines": {
    "safeToModify": [
      "Padding",
      "Margin",
      "minHeight",
      "gutterSpace",
      "NormalBg",
      "HoverBg",
      "NormalBorder",
      "HoverBorder",
      "NormalBradius",
      "HoverBradius",
      "NormalBShadow",
      "HoverBShadow",
      "shapeTop",
      "shapeTColor",
      "shapeTWidth",
      "shapeTHeight",
      "shapeBottom",
      "shapeBColor",
      "shapeBWidth",
      "shapeBHeight",
      "customClass",
      "customId",
      "HideDesktop",
      "HideTablet",
      "HideMobile"
    ],
    "requiresValidation": [
      "contentWidth",
      "containerWide",
      "height",
      "tagName",
      "overflow",
      "deepBgopt",
      "videosour",
      "craBgeffect",
      "imgeffect",
      "flexDirection",
      "contSticky",
      "constType",
      "sanimaType",
      "midOption"
    ],
    "dangerous": [
      "block_id",
      "columns",
      "align",
      "className",
      "anchor"
    ]
  },
  "usesContext": [],
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "anchor": true,
    "html": false
  },
  "innerBlocksAllowed": true,
  "parentBlockRequired": false
}