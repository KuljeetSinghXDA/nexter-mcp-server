{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "blockName": "tpgb/tp-form-hidden-field",
  "title": "Hidden Field - Core Attributes",
  "description": "Core content and structure attributes",
  "attributes": {
    "block_id": {
      "type": "string",
      "required": true,
      "description": "Unique 4-character hex identifier for the block"
    },
    "nameValue": {
      "type": "string",
      "default": "Hidden Name",
      "description": "Field name (used as key in form submission data)"
    },
    "actualValue": {
      "type": "string",
      "default": "",
      "description": "Field value (data to be submitted with form)"
    }
  },
  "required": [
    "block_id"
  ],
  "editingGuidelines": {
    "safeToModify": [
      "nameValue",
      "actualValue"
    ],
    "requiresValidation": [],
    "dangerous": [
      "block_id"
    ]
  },
  "usesContext": [],
  "supports": {
    "anchor": true,
    "align": [
      "wide",
      "full"
    ]
  },
  "innerBlocksAllowed": false,
  "parentBlockRequired": true
}