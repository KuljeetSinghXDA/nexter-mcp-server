  "validationRules": {
    "block_id": "Required, unique identifier for the pricing table",
    "style": "Layout style: 'style-1', 'style-2', or 'style-3'",
    "titleStyle": "Title section style: 'style-1' or 'style-2'",
    "title": "Plan name (e.g., 'Professional', 'Enterprise')",
    "subTitle": "Optional subtitle/tagline for the plan",
    "iconType": "Must be 'none', 'icon', or 'img'",
    "iconStore": "Required if iconType='icon', FontAwesome class (e.g., 'fas fa-rocket')",
    "imgStore": "Required if iconType='img', object with url and id",
    "priceStyle": "Price display style: 'style-1', 'style-2', or 'style-3'",
    "preText": "Price prefix (usually currency symbol like '$')",
    "priceValue": "Price amount or custom text (e.g., '49' or 'Contact Sales')",
    "postText": "Price suffix (e.g., '/month', '/year', 'per user')",
    "disPrePrice": "Boolean, show crossed-out previous price for discount",
    "prevPriceValue": "Required if disPrePrice=true, original price before discount",
    "contentStyle": "Feature display: 'stylish' (icon list) or 'wysiwyg' (HTML content)",
    "stylishList": "Array of features with icons, required if contentStyle='stylish'",
    "wyContent": "HTML content, required if contentStyle='wysiwyg'",
    "readMoreToggle": "Boolean, enable collapsed feature list with expand link",
    "showListToggle": "String number, how many features to show before collapse (e.g., '3', '5')",
    "ctaText": "Optional call-to-action text above button",
    "disRibbon": "Boolean, enable ribbon/badge (e.g., 'Popular', 'Best Value')",
    "ribbonStyle": "Ribbon style: 'style-1', 'style-2', or 'style-3'",
    "ribbonText": "Required if disRibbon=true, badge text",
    "extbtnPosition": "Button position: 'top' or 'bottom'",
    "extBtnAlignment": "Button alignment: responsive object (left/center/right)"
  },
  "warnings": [
    "NEVER modify block_id - it's the unique identifier",
    "iconType='icon' requires iconStore (FontAwesome class)",
    "iconType='img' requires imgStore.url (image URL)",
    "disPrePrice=true requires prevPriceValue or won't show discount",
    "contentStyle='stylish' requires stylishList array or features won't display",
    "contentStyle='wysiwyg' requires wyContent or content area will be empty",
    "readMoreToggle=true requires showListToggle (number of visible items)",
    "disRibbon=true requires ribbonText or ribbon will be empty",
    "stylishList items need iconStore (FontAwesome class) to show icons",
    "Empty title creates plan without header (usually unintentional)",
    "priceValue can be text ('Contact Sales') instead of number for enterprise plans",
    "Missing innerBlocks (button) creates pricing table without CTA",
    "iconStyle='square' or 'rounded' requires background (icnNormalBG) to be visible",
    "Tooltip features (eIcnToggle=true) require tooltipText to display"
  ],
  "commonMistakes": [
    "Forgetting to set openTypography: 1 before setting typography values (titleTypo, priceTypo, etc.)",
    "Using iconType='icon' but not providing iconStore FontAwesome class",
    "Setting disPrePrice=true but forgetting prevPriceValue (discount won't show)",
    "Creating feature list (stylishList) but leaving contentStyle='wysiwyg'",
    "Not setting openBg: 1 before setting icnNormalBG gradient/color",
    "Copying stylishList without updating _key values (should be unique: '0', '1', '2'...)",
    "Using eIcnToggle=true (tooltip icon) but not providing tooltipText",
    "Setting ribbonText but forgetting disRibbon=true (ribbon won't appear)",
    "Using readMoreToggle=true without specifying showListToggle number",
    "Creating too many features (15+) without readMoreToggle - creates overwhelming list",
    "Not testing responsive typography sizes (md/sm/xs)",
    "Using gradient backgrounds without proper contrast for text readability",
    "Forgetting to add button as innerBlock (pricing table needs CTA)"
  ],
  "bestPractices": [
    "Use 'Popular' or 'Best Value' ribbons to guide users to recommended plan",
