        "icnTrigger": "click",
        "menuOSpeed": "400",
        "iconPos": "fixed",
        "bottomAuto": true,
        "bottomASize": {"md": "5", "unit": "%"},
        "rightAuto": true,
        "rightASize": {"md": "5", "unit": "%"},
        "overlayColorTgl": true,
        "overlayColor": "rgba(0, 0, 0, 0.4)",
        "circleMenu": [
          {
            "_key": "0",
            "iconStore": "fas fa-phone",
            "title": "Call Us",
            "linkType": "phone",
            "phoneNo": "+1-555-123-4567",
            "ariaLabel": "Call us at +1-555-123-4567",
            "iconNmlColor": "#25D366"
          },
          {
            "_key": "1",
            "iconStore": "fas fa-envelope",
            "title": "Email",
