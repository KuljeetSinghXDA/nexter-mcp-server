{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "blockName": "tpgb/tp-circle-menu",
  "title": "Circle Menu - Comprehensive Usage Examples",
  "description": "Complete example configurations for circular and straight menu layouts with animations, tooltips, and custom positioning",

  "examples": [
    {
      "name": "Social Media Circle Menu (Bottom-Right Quarter)",
      "description": "Quarter-circle social menu opening bottom-right with 5 social icons",
      "visualDescription": "Fixed bottom-right corner, 90-degree arc, 5 social icons (FB, Twitter, Instagram, LinkedIn, YouTube), 120px radius",
      "complexity": "simple",
      "attributes": {
        "block_id": "cm001",
        "layoutType": "circle",
        "cDirection": "bottom-right",
        "menuStyle": "style-1",
        "angleStart": "0",
        "angleEnd": "90",
        "circleRadius": {"md": "120", "unit": "px"},
        "icnTrigger": "hover",
        "menuOSpeed": "500",
        "iconPos": "fixed",
        "bottomAuto": true,
        "bottomASize": {"md": "3", "unit": "%"},
        "rightAuto": true,
        "rightASize": {"md": "3", "unit": "%"},
        "circleMenu": [
          {
            "_key": "0",
            "iconType": "icon",
            "iconStore": "fab fa-facebook-f",
            "title": "Facebook",
            "linkType": "url",
            "linkUrl": {"url": "https://facebook.com/yourpage", "target": "_blank", "nofollow": "nofollow"},
            "ariaLabel": "Visit our Facebook page",
            "iconNmlColor": "#4267B2",
            "iconHvrColor": "#365899"
          },
          {
            "_key": "1",
            "iconStore": "fab fa-twitter",
            "title": "Twitter",
            "linkType": "url",
            "linkUrl": {"url": "https://twitter.com/yourhandle", "target": "_blank"},
            "iconNmlColor": "#1DA1F2"
          },
          {
            "_key": "2",
            "iconStore": "fab fa-instagram",
            "title": "Instagram",
            "linkType": "url",
            "linkUrl": {"url": "https://instagram.com/yourprofile", "target": "_blank"},
            "iconNmlColor": "#E4405F"
          },
          {
            "_key": "3",
            "iconStore": "fab fa-linkedin-in",
            "title": "LinkedIn",
            "linkType": "url",
            "linkUrl": {"url": "https://linkedin.com/company/yourcompany", "target": "_blank"},
            "iconNmlColor": "#0077B5"
          },
          {
            "_key": "4",
            "iconStore": "fab fa-youtube",
            "title": "YouTube",
            "linkType": "url",
            "linkUrl": {"url": "https://youtube.com/@yourchannel", "target": "_blank"},
            "iconNmlColor": "#FF0000"
          }
        ]
      },
      "useCase": "Social media links, brand presence, blog sidebars, footer floating buttons"
    },

